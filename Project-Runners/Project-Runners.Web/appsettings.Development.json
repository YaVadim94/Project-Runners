{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "AllowedHosts": "*"
  },
  "RabbitMQ": {
    "hostName": "localhost",
    "port": 5672,
    "userName": "myuser",
    "password": "mypassword"
  },
  "Redis": "localhost:6379"
}
